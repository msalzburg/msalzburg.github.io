---
interface Props {
  entries: NavigationEntryData[];
}

const { entries } = Astro.props;
import NavigationEntry from "@components/navigation/NavigationEntry.astro";
---

<div class="bg-gray-500 pt-3 pb-3 pl-5 flex flex-row space-x-3">
  {entries.map((entry) => <NavigationEntry entry={entry} />)}
</div>
